{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:import/typescript",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "types": 0
  }
}
